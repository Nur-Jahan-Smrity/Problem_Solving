//LOJ_Triangle_Partitioning_1043
#include<bits/stdc++.h>

  // reverse(s.begin(), s.end());
using namespace std;
typedef long long ll;
//int j=0;
ll i=1;
void solve()
{
    double ab,ac,bc,ratio;
  
    cin>>ab>>ac>>bc>>ratio;
    double ad=ab*(sqrt(ratio/(ratio+1)));
    cout<<"Case "<<i<<": "<<fixed<<setprecision(10)<<ad<<endl;
    i++;
}
void test()
{
    long long t;
    cin>>t;

    while(t--)
    {
        //j++;
        solve();
    }

}

int main()
{

    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    //int n = sizeof(arr) / sizeof(arr[0]);
    test();
   // solve();
}
